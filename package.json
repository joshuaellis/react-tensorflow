{
  "name": "react-tensorflow",
  "version": "0.1.5",
  "description": "TensorflowJS based hooks for react.js",
  "files": [
    "/dist"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "jsnext:main": "./dist/index.es.js",
  "types": "./dist/src/index.d.ts",
  "repository": "https://github.com/joshuaellis/react-tensorflow.git",
  "author": "Josh Ellis <joshua.ellis18@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "tensorflow",
    "hooks",
    "react-hooks"
  ],
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c && tsc-alias",
    "postbuild": "node ./scripts/postbuild",
    "pretest": "rm -rf ./coverage",
    "prepublishOnly": "yarn test --forceExit && yarn build",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "ignore": [
      "/coverage/"
    ],
    "global": [
      "test",
      "describe",
      "it",
      "expect",
      "jest"
    ]
  },
  "bugs": "https://github.com/joshuaellis/react-tensorflow/issues",
  "peerDependencies": {
    "@tensorflow/tfjs": ">=2.0.0",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@tensorflow/tfjs": "^2.7.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-jest": "^26.6.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "prettier-standard": "^16.4.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "standardx": "^7.0.0",
    "ts-jest": "^26.4.4",
    "tsc-alias": "^1.1.5",
    "typescript": "^4.0.5"
  }
}
